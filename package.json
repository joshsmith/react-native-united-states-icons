{
  "name": "react-native-united-states-icons",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "codemod": "jscodeshift -t ./codemod/convert-svg-to-react-native.ts ./SVG --extensions=svg --outputDir ./src/icons --verbose=2",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --fix . --ext .ts,.tsx",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "prepare": "npm run build",
    "start": "node ./dist/index.js",
    "typecheck": "tsc --noEmit",
    "version": "npm run build && git add -A dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshsmith/react-native-united-states-icons.git"
  },
  "keywords": [
    "React Native",
    "SVG",
    "Icons",
    "Icon",
    "State",
    "States",
    "State Icons",
    "US",
    "USA",
    "United States",
    "United States Icons",
    "United States of America",
    "America"
  ],
  "author": "Josh Smith",
  "license": "MIT",
  "react-native": "dist/index.esm.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "dependencies": {
    "jscodeshift": "^17.0.0",
    "react": "^18.3.1",
    "react-native-svg": "^15.8.0",
    "svgson": "^5.3.1",
    "tslib": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "prettier-plugin-sort-imports": "^1.8.6",
    "rollup": "^4.24.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  }
}
